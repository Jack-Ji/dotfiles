{
    "inlayHint.enable": true,
    "semanticTokens.enable": true,
    "languageserver": {
        "zls" : {
            "command": "zls",
            "filetypes": ["zig", "zon"]
        },
        "go": {
            "command": "gopls",
            "rootPatterns": ["go.mod"],
            "trace.server": "verbose",
            "filetypes": ["go"]
        },
        "swift": {
            "command": "sourcekit-lsp",
            "filetypes": ["swift"]
        }
    }
}

